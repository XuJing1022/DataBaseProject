create database cs;
show databases;
use cs;
create table course (id int, name char(10), score float, primary key (id));
show tables;
insert into course values (111, 'database', 222.2);
insert into course values (222, 'math', 555.2);
insert into course values (222, 'math', 333.2);
insert into course values (333, 'english', 333.2);
insert into course values (234, 'network', 111.2);
select * from course;
select * from course where score > 111.2;
create index courseid on course (id);
insert into course values (444, 'os', 444.2);
select * from course;
update course set id = 1024 where id = 333;
select * from course;
drop index courseid;
select * from course;